package com.xworkz.State;

import com.xworkz.State.dao.StateDAO;
import com.xworkz.State.dao.StateDAOImpl;
import com.xworkz.State.entity.StateEntity;

public class StateOmicron {

	public static void main(String[] args) {
		StateEntity countryEntity1 = new StateEntity(8,"scotland", 34, "54.5 lakhs");
		StateEntity countryEntity2= new StateEntity(9,"singapore", 23, "56.9 lakhs");
		StateEntity countryEntity3= new StateEntity(10,"switzerland", 15, "86.4 lakhs");
		StateEntity countryEntity4= new StateEntity(11,"switzerland", 15, "86.4 lakhs");
		
		StateDAO dao = new StateDAOImpl();
		dao.create(countryEntity1);
		dao.create(countryEntity2);
		dao.create(countryEntity3);
		dao.create(countryEntity4);
		

	}

}
